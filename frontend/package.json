{
  "name": "@fyndra/frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "build:tailwind": "tailwindcss -i ./src/assets/styles/input.css -o ./src/assets/styles/output.css",
    "watch:tailwind": "tailwindcss -i ./src/assets/styles/input.css -o ./src/assets/styles/output.css --watch"
  },
  "dependencies": {
    "@fyndra/shared": "workspace:*",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.17.16",
    "framer-motion": "^12.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
